{
    "name": "minh261002/momo-payment",
    "type": "library",
    "description": "Thư viện hổ trợ tích hợp cổng thanh toán MoMo.",
    "homepage": "https://github.com/minh261002/MomoPayment",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Tran Cong Minh",
            "email": "trcongminh.261002@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "omnipay/common": ">=3.2",
        "php-http/guzzle7-adapter": "^1.0"
    },
    "require-dev": {
        "http-interop/http-factory-guzzle": "^1.2",
        "omnipay/tests": ">=4.0",
        "scrutinizer/ocular": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Omnipay\\MoMo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omnipay\\MoMo\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "\"vendor/bin/phpunit\""
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
	}
}
